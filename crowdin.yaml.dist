project_identifier: catrobat
api_key: #ask the crowdin coordinator for key
base_url: http://api.crowdin.net
preserve_hierarchy: true

files:
  -
    source: '/app/Resources/translations/catroweb.en.yml'                                   #source files filter
    translation: '/app/Resources/translations/catroweb.%locale_with_underscore%.yml'  #where translations live
    update_option: 'update_as_unapproved'
    languages_mapping:
      locale_with_underscore:
        'ar': 'ar'
        'bs': 'bs'
        'da': 'da'
        'de': 'de'
        'es-ES': 'es'
        'fa': 'fa'
        'fr': 'fr'
        'gu-IN': 'gu'
        'hi': 'hi'
        'hr': 'hr'
        'hu': 'hu'
        'it': 'it'
        'ja': 'ja'
        'ms': 'ms'
        'ko': 'ko'
        'nl': 'nl'
        'no': 'no'
        'pl': 'pl'
        'ps': 'ps'
        'pt-PT': 'pt'
        'ro': 'ro'
        'ru': 'ru'
        'sl': 'sl'
        'sr': 'sr'
        'sv-SE': 'sv'
        'ta': 'ta'
        'th': 'th'
        'tr': 'tr'
        'ur-PK': 'ur'
        'zh-CN': 'zh'